## user.graphqls ===============================================

type User implements Node{
  id: ID!
  name: String
  email: String
  emailVerified: [Timestamp]
  image: String
  works: WorkPagination
  profile: Profile
}

type UserPagination implements Pagination{
  type: Model!
  pageInfo: PaginationInfo!
  nodes: [User!]!
}
