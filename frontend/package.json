    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.8.1",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/sinon": "^10.0.13",
    "babel-plugin-import": "^1.13.5",
    "eslint": "^8.35.0",
    "eslint-config-next": "^13.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
