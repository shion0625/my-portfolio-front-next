{
  "name": "my-portfolio-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "local": "APP_ENV=local next dev",
    "dev": "APP_ENV=development next dev",
    "pro": "APP_ENV=production next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "eslint src --ext .js,jsx,.ts,.tsx --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,json,css}' --ignore-path ../.gitignore",
    "format:check": "prettier --check './**/*.{js,jsx,ts,tsx,json,css}' --ignore-path ../.gitignore",
    "test": "jest --watch",
    "test:ci": "jest",
    "test-diff": "jest -o",
    "test-diff-watch": "jest --watch",
    "test-coverage": "jest --coverage",
    "test-coverage-json": "jest --collectCoverage=true --coverageReporters=json-summary"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@hookform/error-message": "^2.0.0",
    "@mui/icons-material": "^5.10.9",
    "@mui/lab": "^5.0.0-alpha.104",
    "@mui/material": "^5.10.10",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.6.1",
    "@tanstack/react-virtual": "^3.0.0-beta.47",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "next": "^13.2.3",
    "next-auth": "^4.16.4",
    "nookies": "^2.5.2",
    "prisma": "^4.6.1",
